alias g='git'
alias gb='git branch'
alias gs='git status'
alias gac='git add . && git commit -m'
alias gq='git add . && git commit -m "GQ" && git push origin master'
alias gc='git checkout'

alias gFileReset='git checkout --'
alias gPushMaster='git push origin master'
alias gPullMaster='git pull origin master'
alias gFileUndoAdd='git reset HEAD'

alias gUpdateSubmodules='git submodule update --recursive --remote'
alias gUntrackRemote='git branch --unset-upstream'
